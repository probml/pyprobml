    	dh_1 [label="\\dot{h}_1"];
    }
    
    subgraph cluster1{
	label = "Pipe";  
	node [texmode="math"]
	c_5--h_1; c_5--q_12;
	q_12 [label="q_{12}"];
    }
    
    c_5--h_2;
    c_3--q_12;
    c_6--q_12;
    
    subgraph cluster2{
    	label = "Tank 2";
    	node [texmode="math"]
    	c_6--dh_2;c_6--q_2;
    	d_7--dh_2; d_7--h_2;
    	c_8--h_2;c_8--q_2;
    	c_m--q_2;
    	dh_2 [label="\\dot{h}_2"];
    }
