    val = minimize(lambda x: -entropy(x), 
                   x0=[1/C]*C, 
                   bounds=[(0., 1.)] * C,
                   constraints=c)['x']
    max_ent.append(entropy(val))
    plt.plot(xs, val, 'o--', lw=2.5, label=names[i])
